        <input type="hidden" name="selectedFieldsSearch" value="{{selectedFieldsSearch}}">
        <input name="username" id="username" class="form-control mr-sm-2" type="search" placeholder="{{insusername}}">
        <button class="btn btn-outline-success my-2 my-sm-0" type="submit">{{search}}</button>
      </form>
    </nav>
  </div>
</body>
</html>
